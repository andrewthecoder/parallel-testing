{# ParallelTestingBundle/Resources/views/Default/charts.html.twig #}

{% extends "OryzoneBoilerplateBundle::html5.html.twig" %}

{% block head_title %}Parallel Testing: Single CPU Results{% endblock %}

{% block body_container_header %}
	<style type='text/css'>
		html, body, #container {
			height: 100%;
			width: 100%;
			margin: 0;
			padding: 0;
			position: relative;
			overflow: hidden;
			scroll: none;
		}
		#container > div {
			height: 90%;
		}
	</style>
    <h1>Parallel Testing: Single CPU Results</h1>
{% endblock %}

{% block body_container_main %}
	<div id="linechart" style="position: relative; min-width: 400px; height: 100%; margin: 0 auto"></div>
{% endblock %}

{% block body_container_footer %}
{% endblock %}

{% block body_js %}
	<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js" type="text/javascript"></script>

	<script src="{{ asset('bundles/obhighcharts/js/highcharts/highcharts.js') }}"></script>
	<script src="{{ asset('bundles/obhighcharts/js/highcharts/modules/exporting.js') }}"></script>
	<script type="text/javascript">
	{{ chart(chart) }}
	</script>
{% endblock %}